{
	"name": "jeroen/file-fetcher",
	"type": "library",
	"description": "Simple file fetching interface",
	"keywords": [
		"file_get_contents",
		"http",
		"interface",
		"test doubles",
		"mocks",
		"spies",
		"stubs",
		"file fetcher",
		"wget",
		"curl",
		"request",
		"http request",
		"http client",
		"cache"
	],
	"homepage": "https://github.com/JeroenDeDauw/FileFetcher",
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Jeroen De Dauw",
			"email": "jeroendedauw@gmail.com",
			"homepage": "http://jeroendedauw.com",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=7.1",
		"jeroen/simple-cache": "~2.0",
		"psr/log": "~1.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~7.2",
		"squizlabs/php_codesniffer": "~3.2",
		"phpmd/phpmd": "~2.6",
		"ockcyp/covers-validator": "~1.0",
		"wmde/psr-log-test-doubles": "~2.1",
		"phpstan/phpstan": "~0.10.1",
		"symfony/stopwatch": "~4.2"
	},
	"autoload": {
		"psr-4": {
			"FileFetcher\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"FileFetcher\\Tests\\": "tests/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "4.6.x-dev"
		}
	}
}
