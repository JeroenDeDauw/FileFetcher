{
	"name": "jeroen/file-fetcher",
	"type": "library",
	"description": "Simple file fetching interface",
	"keywords": [
		"file fetcher",
		"file_get_contents",
		"cache",
		"wget",
		"curl"
	],
	"homepage": "https://github.com/JeroenDeDauw/FileFetcher",
	"license": "GPL-2.0-or-later",
	"authors": [
		{
			"name": "Jeroen De Dauw",
			"email": "jeroendedauw@gmail.com",
			"homepage": "http://jeroendedauw.com",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=7.1",
		"jeroen/simple-cache": "~2.0",
		"psr/log": "~1.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~6.1",
		"squizlabs/php_codesniffer": "~3.2",
		"phpmd/phpmd": "~2.6",
		"ockcyp/covers-validator": "~1.0",
		"wmde/psr-log-test-doubles": "~2.1",
		"phpstan/phpstan": "~0.9.2"
	},
	"autoload": {
		"psr-4": {
			"FileFetcher\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"FileFetcher\\Tests\\": "tests/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "4.4.x-dev"
		}
	}
}
